{
  "name": "planton-backend",
  "version": "1.0.0",
  "description": "PlantOn Gateway Backend API",
  "author": "PlantOn Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.js --passWithNoTests",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json --passWithNoTests",
    "migrate": "dotenv -e .env.development -- prisma migrate dev",
    "migrate:test": "dotenv -e .env.test -- prisma migrate dev",
    "db:generate": "dotenv -e .env.development -- prisma generate",
    "db:push": "dotenv -e .env.development -- prisma db push",
    "db:reset": "dotenv -e .env.development -- prisma migrate reset",
    "db:studio": "dotenv -e .env.development -- prisma studio"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@faker-js/faker": "^9.9.0",
    "@jest/globals": "^30.0.4",
    "@jest/types": "^30.2.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/websockets": "^10.3.0",
    "@nodeteam/nestjs-pipes": "^1.5.1",
    "@nodeteam/nestjs-prisma-pagination": "^1.0.6",
    "@prisma/client": "^5.8.1",
    "@ucast/sql": "1.0.0-alpha.1",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^5.8.1",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4",
    "ws": "^8.16.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.5",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.10",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^9.0.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
